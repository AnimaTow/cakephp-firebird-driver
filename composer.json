{
    "name": "animatow/cakephp-firebird-driver",
    "description": "CakePHP 4 Driver for Firebird Database",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp",
        "cakephp4",
        "firebird",
        "database",
        "driver"
    ],
    "homepage": "https://github.com/animatow/cakephp-firebird-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Stephan Broeker",
            "homepage": "https://twitter.com/animatow/",
            "role": "Developer"
        },
        {
            "name": "Kevin Gledhill",
            "homepage": "https://mediafelis.de/",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/animatow/cakephp-firebird-driver/issues",
        "source": "https://github.com/animatow/cakephp-firebird-driver"
    },

    "config": {
        "preferred-install": "src"
    },
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "CakephpFirebird\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpFirebird\\Test\\": "tests"
        }
    }
}
